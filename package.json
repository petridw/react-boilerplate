  "license": "ISC",
  "dependencies": {
    "express": "^4.13.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "nodemon": "^1.8.1",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "watchify": "^3.6.0"
  }
}
