{
  "name": "react-browserify",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "jshintConfig": {
    "esnext": true
  },
  "scripts": {
    "start": "node index.js",
    "start-dev": "npm run watch-js & nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-js": "browserify src/js/app.js -o client/build/bundle.js -t [ babelify --presets [ react es2015 ] ]",
    "watch-js": "watchify src/js/app.js -o client/build/bundle.js -t [ babelify --presets [ react es2015 ] ] -dv"
  },
